{
  "name": "coloring-book-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run tests/unit --reporter=verbose",
    "test:system": "vitest run tests/system --reporter=verbose",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "test:all": "vitest run tests/",
    "test:watch": "vitest tests/",
    "test:coverage": "vitest run --coverage tests/",
    "test:ci": "vitest run --reporter=json tests/ > test-results.json",
    "test:mvp": "ts-node tests/test-runner.ts mvp",
    "test:full": "ts-node tests/test-runner.ts comprehensive",
    "test:suite": "npm run build && npm run test:full",
    "discover-adapter": "ts-node --transpile-only lib/adapter-discovery.ts discover",
    "list-adapters": "ts-node --transpile-only lib/adapter-discovery.ts list",
    "test-adapter": "ts-node --transpile-only lib/adapter-discovery.ts test",
    "view-adapter": "ts-node --transpile-only lib/adapter-discovery.ts view",
    "remove-adapter": "ts-node --transpile-only lib/adapter-discovery.ts remove"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "axios": "^1.6.0",
    "inquirer": "^13.2.2",
    "next": "^15.0.0",
    "node-html-parser": "^6.1.13",
    "pdf-lib": "^1.17.0",
    "playwright": "^1.58.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@pulumi/aws": "^6.20.0",
    "@pulumi/awsx": "^2.10.0",
    "@pulumi/pulumi": "^3.100.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^15.0.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
